#include <asm/macro.h>

#define HANDLER_(irq) ;\
    ASM_FUNC_DEF(interrupt_handler_##irq): ;\
        push    $irq ;\
        call    generic_handler ;\
        pop     %eax # The irq number
        iret

HANDLER_(0)
HANDLER_(1)
HANDLER_(2)
HANDLER_(3)
HANDLER_(4)
HANDLER_(5)
HANDLER_(6)
HANDLER_(7)
